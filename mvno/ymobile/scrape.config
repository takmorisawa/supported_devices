{
    "html_files":"projects/support_devices/mvno/ymobile/tmp/html",
    "writefile_path":"projects/support_devices/mvno/ymobile/tmp/csv/devices_ymobile-scraped.csv",
    "targets_xpath":"//tr[./td[4]]",
    "column_xpath":
        [   
            ["device_type","normalize-space(./ancestor::div[contains(@class,'tab-target-item')]/@class)"],
            ["maker","normalize-space(./preceding::h3[1])"],
            ["name","./td[1]/text()"],
            ["sim","normalize-space(./td[2])"],
            ["os","normalize-space(./td[3])"],
            ["note","normalize-space(./td[4])"]
        ]
}